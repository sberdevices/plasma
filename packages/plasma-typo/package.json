        "@types/node": "16.7.13",
        "@types/react": "16.9.38",
        "@types/react-dom": "16.9.8",
        "@types/styled-components": "5.1.0",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-scripts": "3.4.4",
        "styled-components": "5.3.1",
        "typescript": "3.9.10",
        "typescript-coverage-report": "0.6.4"
